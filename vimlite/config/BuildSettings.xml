      <Pattern FileNameIndex="-1" LineNumberIndex="-1" Name="Error">undefined reference to</Pattern>
      <Pattern FileNameIndex="-1" LineNumberIndex="-1" Name="Error">\\*\\*\\* \\[[a-zA-Z\\-_0-9 ]+\\] (Error)</Pattern>
      <Pattern FileNameIndex="1" LineNumberIndex="3" Name="Warning">([a-zA-Z:]{0,2}[ a-zA-Z\\.0-9_/\\+\\-]+ *)(:)([0-9]+ *)(:)([0-9:]*)?( warning)</Pattern>
